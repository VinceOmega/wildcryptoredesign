<section class="section -promo col-lg-12 col-md-12 col-sm-12 col-lg-12">

    <div class="-width--px1366">

        <div class="div -promo--container col-lg-4 col-md-6 col-sm-12 col-xs-12">

            <div class="div -promo--header col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <h2 class="h2 -promo--h2">JOIN NOW AND WIN BIG!</h2>
            </div>

            <div class="div -promo--left col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="div -promo--text">
                    <div class="div -promo--header -small"><span>-</span><em>sign up and get</em><span>-</span></div>
                    <div class="div -promo--header -huge">20</div>
                    <div class="div -promo--header -large" >FREE SPINS</div>
                    <div class="div -promo--header -medium -honeymustard" >NO DEPOSIT REQUIRED</div>
                    <a href="/sign-up" class="input button -btn">Sign Up</a>
                </div>
            </div>

            <%--             <form id="new_user" method="post" class="form -promo--right col-lg-6 col-md-6 col-sm-12 col-xs-12">

               <span class="email-field-wrap">
                    <input class="email-field " type="text" value="" translate=""   placeholder="Email" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;">
                </span>

                <span class="username-field-wrap">
                    <input class="username-field" type="text" value="" translate=""   placeholder="Username">
                </span>

                <span class="pass-field-wrap">
                    <input id="user_password" class="pass-field" type="password" translate=""  placeholder="Password" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;">
                </span>

                <span class="pass-field-wrap ng-hide">
                    <input id="user_password_confirmation" class="pass-field" type="password" translate="" placeholder="Password confirmation" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;">
                </span>

                <div class="currency-field-wrap">
                    <div class="dropdown">
                        <label class="form-label currency-label ng-scope">Currency</label>
                        <button class="form-control dropdown-toggle labeled-control" data-toggle="dropdown" type="button">
                            <span class="name ng-binding">Currency</span>
                            <span class="caret"></span>
                        </button>

                        <ul class="dropdown-menu flags-dropdown" role="menu" class="" >
                            <li class="">

                                <a class="dropdown-menu-item" href="">
                                    <span class="name">BTC</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>

                <div class="checkbox fast-reg-tnc">

                    <input class="input -checkbox" type="checkbox">
                    <span class="span -label">I accept the <a href="/terms-and-conditions" target="_blank" class="">terms and conditions</a></span>

                </div>

            </form> --%>

        </div>

        <div class="div -promo--container -promo--container__left col-lg-4 col-6 col-sm-12 col-xs-12">
            <% if $Game %>
                <% with $Game %>
                        <a class="iwgbox iwgbox--big"<% if $uniqueKey %> href="{$Link}"<% end_if %>>
                            <div class="iwgbox__splashboxholder">
                                <div class="iwgbox__splashbox">
                                    <img class="iwgbox__splash" src="$Splash" alt="{$name} Splash Art" />
                                </div>
                            </div>
                            <div class="iwgbox__caption">
                                <% if enabled %>
                                    <div class="iwgbox__price">{$Top.forceDecimal($Price)} WILD</div>
                                    <div class="button secondarycolour button--thin button--secondary">{$Top.i18n("Homepage__PlayTry")}</div>
                                <% end_if %>
                            </div>
                            <% if $PlaysAvailable > 0 %>
                                <div class="iwgbox__subcaption">$PlaysAvailable play<% if $PlaysAvailable > 1 %>s<% end_if %> available</div>
                            <% end_if %>
                        </a>
                <% end_with %>
            <% end_if %>
        </div>
        
    </div>

</section>